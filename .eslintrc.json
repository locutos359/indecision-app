{
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": [
            "error",
            {
                "code": 120
            }
        ],
        "eqeqeq": [
            "error",
            "always"
        ],
        "no-console": "off",
        "no-undef": "warn",
        "no-unused-vars":"warn",
        "react/react-in-jsx-scope": "off"
    },
    "env": {
        "browser": true,
        "node": true
    }
}